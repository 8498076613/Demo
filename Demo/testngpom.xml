      <class name="com.pom.test.HomePageTest"/>
    
     
      <class name="com.pom.test.CartPageTest"/>
      <class name="com.pom.test.OrderonfirmationPageTest"/>
      
      <class name="com.pom.test.ProductPageTest"/>
     
    </classes>
  </test> <!-- Test -->
